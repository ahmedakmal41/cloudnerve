{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/admin/*",
            "/assets/*"
        ]
    },
    "mimeTypes": {
        ".json": "application/json",
        ".css": "text/css",
        ".js": "text/javascript",
        ".mp4": "video/mp4",
        ".png": "image/png",
        ".jpg": "image/jpeg",
        ".svg": "image/svg+xml",
        ".webp": "image/webp"
    },
    "globalHeaders": {
        "cache-control": "public, max-age=3600"
    }
}