{
    "routes": [
        {
            "route": "/admin",
            "redirect": "/admin/index.html",
            "statusCode": 301
        },
        {
            "route": "/admin/",
            "serve": "/admin/index.html",
            "statusCode": 200
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/admin/*",
            "/assets/*",
            "/*.{css,js,json,png,jpg,jpeg,svg,mp4,webp,ico,woff,woff2,ttf,otf}"
        ]
    },
    "mimeTypes": {
        ".json": "application/json",
        ".css": "text/css",
        ".js": "text/javascript",
        ".mp4": "video/mp4",
        ".png": "image/png",
        ".jpg": "image/jpeg",
        ".svg": "image/svg+xml"
    },
    "globalHeaders": {
        "cache-control": "public, max-age=3600"
    }
}